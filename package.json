{
  "name": "uploader",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "webpack",
    "release:build": "webpack -p",
    "release:clean": "rimraf ./release",
    "release:cpapp": "cpx \"./{app.js,package*,bin/*,public/*.{html,css,js},server/**/*.js}\" ./release/upapp/uploader",
    "release:cpprd": "cpx \"./production/{*,**/*}\" ./release",
    "release": "npm-run-all -s release:clean release:build -p release:cpapp release:cpprd"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "file-saver": "^1.3.3",
    "jsrsasign": "^8.0.3",
    "morgan": "~1.8.1",
    "multer": "^1.3.0",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "serve-favicon": "^2.4.3",
    "sqlite3": "^3.1.8",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^2.29.0",
    "npm-run-all": "^4.0.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1"
  }
}
